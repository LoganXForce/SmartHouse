<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fonfo">

    <TextView
        android:id="@+id/text_slideshow"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:textAlignment="center"
        android:textColor="#00FFFFFF"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Planta Alta"
        android:textColor="#FFFFFF"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="56dp"
        android:layout_marginEnd="36dp"
        android:layout_marginRight="36dp"
        android:text="Puerta:"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/textView6"
        app:layout_constraintEnd_toStartOf="@+id/btnPabierto"
        app:layout_constraintHorizontal_bias="0.851"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/btnPabierto"
        android:layout_width="120dp"
        android:layout_height="54dp"
        android:layout_marginTop="112dp"
        android:background="@drawable/obalito"
        android:text="Abierto"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.457"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnPcerrado"
        android:layout_width="120dp"
        android:layout_height="54dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/obalito"
        android:text="Cerrado"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/btnEoff"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.315" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="52dp"
        android:text="Luz de Sala:"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintEnd_toStartOf="@+id/btnSon"
        app:layout_constraintHorizontal_bias="0.909"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btnSon"
        android:layout_width="120dp"
        android:layout_height="54dp"
        android:layout_marginBottom="76dp"
        android:background="@drawable/obalito"
        android:text="Encendido"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/btnEon"
        app:layout_constraintEnd_toEndOf="@+id/btnPabierto"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btnPabierto" />

    <Button
        android:id="@+id/btnSoff"
        android:layout_width="120dp"
        android:layout_height="54dp"
        android:layout_marginBottom="76dp"
        android:background="@drawable/obalito"
        android:text="Apagado"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/btnEoff"
        app:layout_constraintEnd_toEndOf="@+id/btnPcerrado"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btnPcerrado" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="40dp"
        android:text="Luz Entrada: "
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/textView8"
        app:layout_constraintEnd_toStartOf="@+id/btnEon"
        app:layout_constraintHorizontal_bias="0.882"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btnEon"
        android:layout_width="120dp"
        android:layout_height="54dp"
        android:layout_marginBottom="60dp"
        android:background="@drawable/obalito"
        android:text="Encendido"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/btnLon"
        app:layout_constraintEnd_toEndOf="@+id/btnSon"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btnSon" />

    <Button
        android:id="@+id/btnEoff"
        android:layout_width="120dp"
        android:layout_height="54dp"
        android:layout_marginBottom="60dp"
        android:background="@drawable/obalito"
        android:text="Apagado"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/btnLoff"
        app:layout_constraintEnd_toEndOf="@+id/btnSoff"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btnSoff" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="54dp"
        android:layout_height="26dp"
        android:layout_marginEnd="40dp"
        android:layout_marginRight="40dp"
        android:layout_marginBottom="36dp"
        android:text="Luz 1:"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/textView9"
        app:layout_constraintEnd_toStartOf="@+id/btnLon"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btnLon"
        android:layout_width="120dp"
        android:layout_height="54dp"
        android:layout_marginBottom="64dp"
        android:background="@drawable/obalito"
        android:text="Encendido"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/btnAon"
        app:layout_constraintEnd_toEndOf="@+id/btnEon"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btnEon" />

    <Button
        android:id="@+id/btnLoff"
        android:layout_width="120dp"
        android:layout_height="54dp"
        android:layout_marginBottom="64dp"
        android:background="@drawable/obalito"
        android:text="Apagado"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/btnAoff"
        app:layout_constraintEnd_toEndOf="@+id/btnEoff"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btnEoff" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginLeft="25dp"
        android:layout_marginBottom="56dp"
        android:text="Aire:"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/textView10"
        app:layout_constraintEnd_toStartOf="@+id/btnAon"
        app:layout_constraintHorizontal_bias="0.488"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btnAon"
        android:layout_width="124dp"
        android:layout_height="54dp"
        android:layout_marginBottom="68dp"
        android:background="@drawable/obalito"
        android:text="Encendido"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/btnA2on"
        app:layout_constraintEnd_toEndOf="@+id/btnLon"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btnLon" />

    <Button
        android:id="@+id/btnAoff"
        android:layout_width="124dp"
        android:layout_height="54dp"
        android:layout_marginBottom="68dp"
        android:background="@drawable/obalito"
        android:text="Apagado"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/btnA2off"
        app:layout_constraintEnd_toEndOf="@+id/btnLoff"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btnLoff" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginBottom="108dp"
        android:text="Aire 2:"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/btnA2on"
        app:layout_constraintHorizontal_bias="0.591"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btnA2on"
        android:layout_width="124dp"
        android:layout_height="54dp"
        android:layout_marginBottom="96dp"
        android:background="@drawable/obalito"
        android:text="Encendido"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/btnAon"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/btnAon" />

    <Button
        android:id="@+id/btnA2off"
        android:layout_width="124dp"
        android:layout_height="54dp"
        android:layout_marginBottom="96dp"
        android:background="@drawable/obalito"
        android:text="Apagado"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/btnAoff"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btnAoff" />
</androidx.constraintlayout.widget.ConstraintLayout>